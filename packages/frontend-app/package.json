{
  "name": "frontend-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "babel": "BABEL_ENV=node babel src --extensions \".ts,.tsx\" --ignore \"**/*.d.ts\" --out-dir dist/ --copy-files",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "dev:watch-server": "nodemon --watch dist --exec \"node dist/server/index.js\"",
    "dev:web": "webpack build",
    "dev:watch-web": "webpack build --watch",
    "dev:node": "yarn run babel --delete-dir-on-start",
    "dev:watch-node": "yarn run babel --watch",
    "dev": " export $(cat .env) && npm-run-all dev:node dev:web  --parallel dev:watch-** type-check:watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-react-display-name": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/lodash": "^4.14.188",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/serialize-javascript": "^5.0.2",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.2",
    "dotenv-webpack": "^8.0.1",
    "express": "^4.18.1",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.1",
    "sass-loader": "^13.2.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "@types/react-dom": "^18.0.6",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "final-form": "^4.20.7",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.4.2",
    "sass": "^1.56.1",
    "serialize-javascript": "^6.0.0",
    "zod": "^3.19.1"
  }
}
