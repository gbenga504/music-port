{
  "name": "frontend-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev:cleanup-dist": "rm -rf dist",
    "dev:watch-server": "nodemon --watch dist --exec \"node dist/server/index.js\"",
    "dev:web": "webpack build",
    "dev:watch-web": "webpack build --watch",
    "dev:node": "tsc --build",
    "dev:watch-node": "tsc --build --watch",
    "dev": "npm-run-all dev:cleanup-dist dev:web dev:node --parallel dev:watch-**",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@loadable/webpack-plugin": "^5.15.2",
    "@types/express": "^4.17.14",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2"
  }
}
