{
  "name": "frontend-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production npm-run-all dev:node dev:web",
    "babel": "BABEL_ENV=node babel src --extensions \".ts,.tsx\" --ignore \"**/*.d.ts\" --out-dir dist/ --copy-files",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "dev:watch-server": "nodemon --watch dist --exec \"node dist/server/index.js\"",
    "dev:web": "webpack build",
    "dev:watch-web": "webpack build --watch",
    "dev:node": "yarn run babel --delete-dir-on-start",
    "dev:watch-node": "yarn run babel --watch",
    "dev": "export $(cat .dev.env) && npm-run-all dev:node dev:web  --parallel dev:watch-** type-check:watch",
    "prod": "export $(cat .prod.env) && node dist/server/index.js",
    "test:unit": "jest --testMatch '**/*.test.jest.ts' --passWithNoTests",
    "graphql:typegen": "graphql-codegen --config ./codegen.ts",
    "playwright": "git ls-files -- '*.spec.ts' | xargs npx playwright test",
    "obtain-admin-auth-token": "export $(cat .auth.env) && node -r ts-node/register/transpile-only scripts/obtain-auth-tokens",
    "obtain-platform-cookies": "export $(cat .auth.env) && node -r ts-node/register/transpile-only scripts/obtain-platform-cookies",
    "type:check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "base:lint": "eslint --cache --ext js --ext ts --ext tsx",
    "lint": "yarn base:lint .",
    "lint:fix": "yarn base:lint --fix .",
    "format": "yarn prettier --write .",
    "format:check": "yarn prettier --check ."
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-react-display-name": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-resolvers": "^3.1.1",
    "@jest/globals": "29.4.3",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.2",
    "@playwright/test": "^1.31.1",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/react-webpack5": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/js-cookie": "^3.0.3",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/lodash": "^4.14.188",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/serialize-javascript": "^5.0.2",
    "@types/wait-on": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "dotenv": "^16.0.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unicorn": "^48.0.1",
    "express": "^4.18.1",
    "jest": "29.4.3",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.1",
    "sass-loader": "^13.2.0",
    "storybook": "^7.3.2",
    "tailwindcss": "^3.2.1",
    "ts-jest": "29.0.5",
    "url-loader": "^4.1.1",
    "wait-on": "^7.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "final-form": "^4.20.7",
    "graphql": "16.6.0",
    "graphql-request": "4.0.0",
    "http-proxy-middleware": "^2.0.6",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "nanoid": "3.3.1",
    "playwright": "^1.33.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.4.2",
    "react-swipeable": "^7.0.0",
    "sass": "^1.56.1",
    "serialize-javascript": "^6.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "zod": "^3.19.1"
  }
}
